<?xml version="1.0" encoding="us-ascii"?>
<commentingstyles>
    <style>
        <name>HTML</name>
        <extentions>
            <extension>htm</extension>
            <extension>html</extension>
        </extentions>
        <longcommentstyles>
            <longcomment>
                <StartString>&lt;!--</StartString>
                <EndString>--&gt;</EndString>
            </longcomment>
        </longcommentstyles>
    </style>
    <style>
        <name>JavaScript</name>
        <extentions>
            <extension>js</extension>
        </extentions>
        <shortcommentstyles>
            <shortcomment>//</shortcomment>
        </shortcommentstyles>
        <longcommentstyles>
            <longcomment>
                <StartString>/*</StartString>
                <EndString>*/</EndString>
            </longcomment>
        </longcommentstyles>
    </style>
    <style>
        <name>CSS</name>
        <extentions>
            <extension>css</extension>
        </extentions>
        <longcommentstyles>
            <longcomment>
                <StartString>/*</StartString>
                <EndString>*/</EndString>
            </longcomment>
        </longcommentstyles>
    </style>
</commentingstyles>